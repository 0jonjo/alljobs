<%= form_with model: job do |form| %>
   <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>
  
  <div>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </div>
  
  <div>
    <%= form.label :skills %><br>
    <%= form.text_area :skills %>
  </div>
  
  <div>
    <%= form.label :salary %><br>
    <%= form.text_field :salary  %>
  </div>
  
  <div>
    <%= form.label :company %><br>
    <%= form.text_field :company  %>
  </div>
  
  <div>
    <%= form.label :level %><br>
    <%= form.text_field :level  %>
  </div>
  
  <div>
    <%= form.label :place %><br>
    <%= form.text_field :place %>
  </div>

  <div>
    <%= form.label :date %><br>
    <%= form.text_field :date %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
